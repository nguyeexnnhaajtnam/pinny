#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Workspace/pinny/node_modules/.pnpm/jest@30.0.5_@types+node@22._f6761a0847352c1e9aa68dd932ac4e4e/node_modules/jest/bin/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/jest@30.0.5_@types+node@22._f6761a0847352c1e9aa68dd932ac4e4e/node_modules/jest/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/jest@30.0.5_@types+node@22._f6761a0847352c1e9aa68dd932ac4e4e/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Workspace/pinny/node_modules/.pnpm/jest@30.0.5_@types+node@22._f6761a0847352c1e9aa68dd932ac4e4e/node_modules/jest/bin/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/jest@30.0.5_@types+node@22._f6761a0847352c1e9aa68dd932ac4e4e/node_modules/jest/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/jest@30.0.5_@types+node@22._f6761a0847352c1e9aa68dd932ac4e4e/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest@30.0.5_@types+node@22._f6761a0847352c1e9aa68dd932ac4e4e/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest@30.0.5_@types+node@22._f6761a0847352c1e9aa68dd932ac4e4e/node_modules/jest/bin/jest.js" "$@"
fi
