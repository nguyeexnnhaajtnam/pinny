#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Workspace/pinny/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_3e1079b7e4b48c79cbef43f9fc1ea86f/node_modules/ts-jest/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_3e1079b7e4b48c79cbef43f9fc1ea86f/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Workspace/pinny/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_3e1079b7e4b48c79cbef43f9fc1ea86f/node_modules/ts-jest/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_3e1079b7e4b48c79cbef43f9fc1ea86f/node_modules:/mnt/d/Workspace/pinny/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
